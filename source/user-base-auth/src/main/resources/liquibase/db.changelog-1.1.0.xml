<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet author="Authentication" id="20250222-1">
        <createTable tableName="db_user_base_nation">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="TEXT"/>
            <column name="type" type="INT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet author="Authentication" id="20250222-2">
        <createTable tableName="db_user_base_customer">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="account_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="birthday" type="DATE"/>
            <column name="gender" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="address" type="VARCHAR(255)"/>
            <column name="province_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="district_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="commune_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet author="Authentication" id="20250222-3">
        <addForeignKeyConstraint
                baseTableName="db_user_base_customer"
                baseColumnNames="account_id"
                referencedTableName="db_user_base_account"
                referencedColumnNames="id"
                constraintName="fk_customer_account"/>
        <addForeignKeyConstraint
                baseTableName="db_user_base_customer"
                baseColumnNames="province_id"
                referencedTableName="db_user_base_nation"
                referencedColumnNames="id"
                constraintName="fk_customer_province"/>

        <addForeignKeyConstraint
                baseTableName="db_user_base_customer"
                baseColumnNames="district_id"
                referencedTableName="db_user_base_nation"
                referencedColumnNames="id"
                constraintName="fk_customer_district"/>

        <addForeignKeyConstraint
                baseTableName="db_user_base_customer"
                baseColumnNames="commune_id"
                referencedTableName="db_user_base_nation"
                referencedColumnNames="id"
                constraintName="fk_customer_commune"/>
    </changeSet>

